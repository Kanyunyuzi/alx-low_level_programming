include "main.h"
include <stdio.h>

/**
 *File name: 1-swap.c
 *Description: Write a function that swaps the values of two integers.
 *Return : always (0)
 */

int main(void)
{
  int a;
  int b;
  a = 98;
  b = 42;
  printf("%d\n", a, b);
  return (0);

}

/**swapping
 */
void swap_int(int *a, int *b);
{

a = &a;
b = &b;
*a = 42;
*b = 98;

printf{"%d\n", a, b);
return (0);

}
